{
  "name": "nyaser-maps-downloader",
  "displayName": "Nyaser Maps Downloader",
  "private": true,
  "version": "2.2.2",
  "type": "module",
  "scripts": {
    "pretauri": "npm run version:sync && node minify.js",
    "tauri": "tauri",
    "tauri:win": "(FOR /F \"usebackq tokens=*\" %i IN (\"src-tauri/.env\") DO @SET %i) && npm run tauri",
    "dev": "set NODE_ENV=development && npm run tauri:win dev",
    "build": "set NODE_ENV=production && npm run tauri:win build",
    "version": "node version.js",
    "version:sync": "node version-sync.js && npm install"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "dotenv-cli": "^10.0.0"
  },
  "dependencies": {
    "cssnano": "^7.1.2",
    "html-minifier-terser": "^7.2.0",
    "terser": "^5.46.0"
  }
}
